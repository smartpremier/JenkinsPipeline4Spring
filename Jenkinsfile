pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello Pipeline'
            }
        }
        stage('build') {
            steps {
                echo 'build Pipeline'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy Pipeline'
            }
        }
        stage('test') {
            steps {
                echo 'test Pipeline'
            }
        }
        stage('release') {
            steps {
                echo 'release Pipeline'
            }
        }
    }
}

